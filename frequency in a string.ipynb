   "source": [
    "def char_frequency(str1):\n",
    "    dict={}\n",
    "    for i in str1:\n",
    "        keys = dict.keys()\n",
    "        if i in keys:\n",
    "            dict[i]+=1\n",
    "        else:\n",
    "            dict[i]=1\n",
    "    return dict\n",
    "print(char_frequency('hullabaloo'))"
   ]
